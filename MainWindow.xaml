<Window x:Class="GraphicEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pook"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="132*"/>
            <ColumnDefinition Width="35*"/>
            <ColumnDefinition Width="627*"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <TabPanel Name="ToolBar" Margin="0,0,0.334,370.667" Grid.ColumnSpan="3" >
        </TabPanel>
        <Canvas x:Name="Canvas" Background="White" HorizontalAlignment="Stretch" Width="Auto" MouseMove="Canvas_MouseMove" MouseDown="Canvas_MouseDown" MouseUp="Canvas_MouseUp" Grid.ColumnSpan="4" Margin="2,50,-1.667,-0.333"/>



    </Grid>
</Window>
